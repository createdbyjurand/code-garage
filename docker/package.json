{
  "name": "meblo-tek.pl",
  "version": "0.0.0",
  "scripts": {
    "docker-compose-build": "docker-compose build",
    "docker-compose-down-all": "docker-compose down --rmi all --volumes --remove-orphans",
    "docker-compose-down": "docker-compose down",
    "docker-compose-up-build": "docker-compose up --build",
    "docker-compose-up": "docker-compose up",
    "docker-container-build-production": "docker build --iidfile assist-frontend-docker-container-build-production-latest-iidfile.sha256 --tag assist-frontend-docker-container-build-production:latest --build-arg ANGULAR_BUILD_NAME_IN_PACKAGE_JSON=ng-build-production .",
    "docker-container-build": "docker build --iidfile assist-frontend-docker-container-build-latest-iidfile.sha256 --tag assist-frontend-docker-container-build:latest .",
    "docker-container-remove": "docker rm assist-frontend-docker-container-run",
    "docker-container-run": "docker run --name assist-frontend-docker-container-run -p 8080:80 assist-frontend-docker-container-build:latest",
    "docker-container-stop": "docker stop assist-frontend-docker-container-run",
    "docker-download-default-conf-from-docker-container": "docker cp assist_frontend_container:/etc/nginx/conf.d/default.conf ./git_ignore/nginx/default.conf",
    "docker-download-nginx-conf-from-docker-container": "docker cp assist_frontend_container:/etc/nginx/nginx.conf ./git_ignore/nginx/nginx.conf",
    "docker-list-all-containers": "docker ps -a",
    "docker-list-all-images": "docker images --all",
    "docker-prune-unused-containers-networks-images-volumes": "docker system prune --all --force --volumes",
    "docker-remove-all-stopped-containers-linux": "docker rm $(docker ps --all --quiet)",
    "docker-remove-all-stopped-containers-windows": "docker ps --all --quiet | % { docker stop $_ }",
    "docker-stop-all-containers-linux": "docker stop $(docker ps --all --quiet)",
    "docker-stop-all-containers-windows-cmd": "FOR /f \"tokens=*\" %i IN ('docker ps --all --quiet') DO docker stop %i",
    "docker-stop-all-containers-windows-powershell": "powershell docker rm @(docker ps --all --quiet)",
    "docker-swagger-pull": "docker pull swaggerapi/swagger-ui",
    "docker-swagger-remove": "docker rm assist-frontend-swagger-docker-container",
    "docker-swagger-run": "docker run --publish 80:8080 --env BASE_URL=/swagger --env SWAGGER_JSON=/swagger.json --volume ./swagger.json:/usr/share/nginx/html/swagger.json --name assist-frontend-swagger-docker-container swaggerapi/swagger-ui",
    "docker-swagger-stop": "docker stop assist-frontend-swagger-docker-container",
    "ng-build-production": "ng build --configuration=production",
    "ng-build-watch": "ng build --watch --configuration development",
    "ng-build": "ng build",
    "ng-e2e": "ng e2e",
    "ng-lint": "ng lint",
    "ng-serve-and-open-hmr": "ng serve -o --hmr",
    "ng-serve-and-open": "ng serve -o",
    "ng-serve": "ng serve",
    "ng-test": "ng test",
    "ng": "ng",
    "node-reinstall-node-modules-from-package-lock-json": "node node_scripts/reinstall-node-modules-from-package-lock-json.node.js",
    "node-reinstall-node-modules": "node node_scripts/reinstall-node-modules.node.js",
    "node-release-version-number-update-for-develop": "node node_scripts/release-version-number-update.node.js --branch=develop",
    "node-release-version-number-update-for-master": "node node_scripts/release-version-number-update.node.js --branch=master",
    "node-update-dependencies": "node node_scripts/update-dependencies.node.js",
    "npm-ci": "npm ci",
    "npm-i": "npm i",
    "npm-outdated": "npm outdated",
    "start": "npm run ng-serve-and-open-hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.12",
    "@angular/cdk": "12.2.12",
    "@angular/common": "12.2.12",
    "@angular/compiler": "12.2.12",
    "@angular/core": "12.2.12",
    "@angular/flex-layout": "12.0.0-beta.35",
    "@angular/forms": "12.2.12",
    "@angular/material": "12.2.12",
    "@angular/platform-browser": "12.2.12",
    "@angular/platform-browser-dynamic": "12.2.12",
    "@angular/router": "12.2.12",
    "@fontsource/material-icons": "4.5.0",
    "@fontsource/roboto": "4.5.1",
    "@mdi/font": "6.4.95",
    "hammerjs": "2.0.8",
    "material-design-icons": "3.0.1",
    "rxjs": "7.4.0",
    "swagger-editor-dist": "3.18.2",
    "swagger-ui": "3.52.5",
    "swagger-ui-dist": "3.52.5",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.12",
    "@angular/cli": "12.2.12",
    "@angular/compiler-cli": "12.2.12",
    "@types/jasmine": "3.10.1",
    "@types/node": "16.11.6",
    "@types/swagger-ui": "3.52.0",
    "codelyzer": "6.0.2",
    "jasmine-core": "3.10.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.6",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "prettier": "2.4.1",
    "protractor": "7.0.0",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  }
}
